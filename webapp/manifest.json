{
	"_version": "1.7.0",
	"sap.app": {
		"id": "bmt.training.routing",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"OData.svc": {
				"uri": "/SAMPLE/V2/(S(xymmchsvxzo4qyetk2fca05p))/OData/OData.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "bmt.training.routing.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "bmt.training.routing.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "None"
				},
				"dataSource": "OData.svc",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "bmt.training.routing.view",
				"controlId": "idApp",
				"controlAggregation": "pages",
				"async": true,
				"bypassed": {
					"target": [
						"notFound"
					]
				}
			},
			"routes": [{
				"pattern": "",
				"name": "products",
				"target": ["products"]
			},{
				"pattern": "Products/{productId}",
				"name": "product",
				"target": ["product"]
			}],
			"targets": {
				"products": {
					"viewName": "Products",
					"viewId": "products",
					"viewLevel": 1,
					"title": "Products"
				},
				"product": {
					"viewName": "ProductDetail",
					"viewId": "product",
					"viewLevel": 2,
					"title": "Product"
				},
				"productNotFound": {
					"viewName": "ProductNotFound",
					"viewId": "productNotFound"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			}
		}
	}
}